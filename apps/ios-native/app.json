{
  "expo": {
    "name": "Nekt",
    "slug": "nekt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "nekt",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0a0f1a"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nektus.app",
      "buildNumber": "3",
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "associatedDomains": [
        "applinks:nekt.us",
        "applinks:www.nekt.us",
        "appclips:nekt.us"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.597818194511-6scsdrns5bvp33444u1l133730hgcnhp"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "tel",
          "mailto",
          "sms",
          "https",
          "http",
          "instagram",
          "fb",
          "twitter",
          "whatsapp",
          "tg"
        ]
      }
    },
    "android": {
      "package": "com.nektus.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "buildReactNativeFromSource": true
          }
        }
      ],
      "expo-apple-authentication",
      [
        "react-native-app-clip",
        {
          "name": "Nekt Clip",
          "bundleIdentifier": "com.nektus.app.Clip",
          "targetSdkVersion": "15.1",
          "groupIdentifier": "group.com.nektus.app",
          "appleSignin": true,
          "entryPoint": "./src/AppClip.tsx",
          "excludedPackages": [
            "@react-native-firebase/app",
            "@react-native-firebase/auth",
            "@react-native-firebase/firestore",
            "@react-native-firebase/storage",
            "expo-contacts",
            "expo-location",
            "expo-sensors",
            "expo-dev-client",
            "expo-auth-session",
            "expo-web-browser",
            "@react-native-community/blur",
            "react-native-qrcode-svg",
            "react-native-markdown-display",
            "react-native-gradients"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d6a7896a-60dc-405c-9917-515edbe480c3"
      }
    },
    "owner": "ajweingart"
  }
}
