{
  "expo": {
    "name": "Nekt",
    "slug": "nekt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "nekt",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0a0f1a"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nektus.app",
      "buildNumber": "18",
      "deploymentTarget": "17.0",
      "googleServicesFile": "./GoogleService-Info.plist",
      "usesAppleSignIn": true,
      "associatedDomains": [
        "applinks:nekt.us",
        "applinks:www.nekt.us",
        "appclips:nekt.us",
        "appclips:www.nekt.us",
        "appclips:nekt.tail768878.ts.net?mode=developer"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Nekt needs access to your photos to set your profile picture",
        "NSBluetoothAlwaysUsageDescription": "Nekt uses Bluetooth to discover nearby friends for contact exchange",
        "NSBluetoothPeripheralUsageDescription": "Nekt uses Bluetooth to share your contact info with nearby friends",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.597818194511-6scsdrns5bvp33444u1l133730hgcnhp"
            ]
          }
        ],
        "LSApplicationQueriesSchemes": [
          "tel",
          "mailto",
          "sms",
          "https",
          "http",
          "instagram",
          "fb",
          "twitter",
          "whatsapp",
          "tg"
        ]
      }
    },
    "android": {
      "package": "com.nektus.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      ["expo-calendar", {
        "calendarPermission": "Nekt reads your calendar to find available meeting times. No event details are stored."
      }],
      ["expo-image-picker", {
        "photosPermission": "Nekt needs access to your photos to set your profile picture"
      }],
      ["react-native-ble-plx", {
        "isBackgroundEnabled": false,
        "modes": ["peripheral", "central"],
        "bluetoothAlwaysPermission": "Nekt uses Bluetooth to discover nearby friends"
      }],
      [
        "react-native-app-clip",
        {
          "name": "Nekt Clip",
          "bundleIdentifier": "com.nektus.app.Clip",
          "deploymentTarget": "17.0",
          "groupIdentifier": "group.com.nektus.app",
          "appleSignin": true,
          "entryPoint": "./AppClip.tsx",
          "excludedPackages": [
            "react-native-reanimated",
            "react-native-worklets",
            "expo-contacts",
            "expo-location",
            "expo-sensors",
            "expo-dev-client",
            "expo-auth-session",
            "expo-web-browser",
            "expo-crypto",
            "expo-file-system",
            "expo-font",
            "expo-keep-awake",
            "expo-asset",
            "react-native-qrcode-svg",
            "react-native-markdown-display",
            "expo-calendar"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d6a7896a-60dc-405c-9917-515edbe480c3"
      }
    },
    "owner": "ajweingart"
  }
}
