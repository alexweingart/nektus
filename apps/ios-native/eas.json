{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "local",
    "requireCommit": false
  },
  "build": {
    "base": {
      "node": "22.12.0",
      "bun": "1.3.5",
      "pnpm": "9.0.0",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EAS_NO_FROZEN_LOCKFILE": "1"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "image": "macos-sequoia-15.6-xcode-26.0",
        "prebuildCommand": "node scripts/generate-codegen.js && node scripts/inject-native-modules.js"
      }
    },
    "production": {
      "extends": "base",
      "distribution": "store",
      "autoSubmit": true,
      "ios": {
        "resourceClass": "m-medium",
        "image": "macos-sequoia-15.6-xcode-26.0",
        "prebuildCommand": "node scripts/generate-codegen.js && node scripts/inject-native-modules.js"
      },
      "env": {
        "FASTLANE_XCODEBUILD_SETTINGS_TIMEOUT": "120",
        "FASTLANE_XCODEBUILD_SETTINGS_RETRIES": "6"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "ajweingart@gmail.com",
        "ascAppId": "6756943273"
      }
    }
  }
}
